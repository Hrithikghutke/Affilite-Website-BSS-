<style type="text/css">
    
    
    
    .customer-feedback-widget {
        background-color: #eaeaea;
        
        width: 100%;
    }

    .customer-feedback-widget::before,
    .customer-feedback-widget::after {
        display: none;
    }

    .custom-feedback-widget-form-container {
        background-color: transparent;
        margin: auto;
        max-width: 100%;
        padding: 20px !important;
    }
    
    

    .customer-feedback-form {
        z-index: 0;
        height: auto;
        margin-bottom: 0;
        text-align: center;
        position: relative;
    
    
    
        padding: 44px;
        background-color: #FFF;
    
    
    }

    .customer-feedback-form.form-disabled .form-submit-button {
        pointer-events: none;
    }

    .customer-feedback-form.form-disabled {
        opacity: 0.5;
    }

    .customer-feedback-form .form-table-column {
        width: 20%;
        text-align: center;
        vertical-align: top;
        padding: 0;
    }

    .form-horizontal-radiobutton-label-row .form-table-column {
    
    
    
        padding-top: 5px;
    
    
    }

    .form-horizontal-radiobutton-label {
        padding: 0;
    }

    .form-horizontal-radiobutton-input-row .form-table-column {
        position: relative;
        z-index: 1;
        padding: 0;
    }

    .form-horizontal-radiobutton-input-row .form-table-column:after {
        content: '';
        border-bottom: 2px solid #ccc;
        width: 100%;
        position: absolute;
        top: 50%;
        left: 50%;
        vertical-align: middle;
        transform: translate(0, -100%);
        z-index: -1;
    }

    .form-horizontal-radiobutton-input-row .form-table-column:last-child:after {
        display: none;
    }

    .form-horizontal-radiobutton-container .a-radio.a-touch-radio {
        margin: auto;
    }

    .form-horizontal-radiobutton-input-row .a-control-row {
        line-height: 0;
    }

    .form-horizontal-radiobutton-input-row .a-radio-fancy label {
        line-height: 19px;
        margin-left: 0;
        padding-left: 0;
        text-indent: 0;
        top: 0;
    }

    .form-horizontal-radiobutton-input-row .a-radio-fancy .a-icon-radio {
        float: left;
        position: absolute;
        left: 50%;
        transform: translate(-50%, 0);
    }

    /* begin common radiobutton/checkbox styles */
    .form-field-container .field-missing {
        text-align: center;
        visibility: hidden;
        height: 25px;
        vertical-align: middle;
    }

    .form-field-container[data-error='empty'] .field-missing {
        visibility: visible;
    }
    /* end common radiobutton/checkbox styles */

    /* begin checkbox styles */
    
    
    
    .form-horizontal-checkbox-container .form-options {
        /* overrides a style elsewhere */
        max-width: 650px;
    }

    .form-horizontal-checkbox-input-row .form-field-answer {
        text-align: left;
        display: inline-block;
        width: auto;
        white-space: nowrap;
        margin: 0 5px;
    }

    .form-horizontal-checkbox-input-row .form-field-answer label {
