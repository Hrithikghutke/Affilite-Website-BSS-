<script type="text/javascript">
    P.when('jQuery', 'ready').execute('customer-feedback-card-form_86cbb23e-964d-4c89-8fd8-941a0cbe32e9', function($) {
        var formSelector = '#form_86cbb23e-964d-4c89-8fd8-941a0cbe32e9';
        var mainFieldSelector = 'input[name="fieldResponses[0].contents[0]"]';
        var setUpNoneOfAbove = '' === 'true';
        var useDifferentiatedResponseMessages = 'true' === 'true';

        if (setUpNoneOfAbove) {
            setUpMultiChoiceNoneOfAbove();
        }

        $(mainFieldSelector, formSelector).click(function() {
            clearValidationMessage(this);
        });

        $('.form-submit-button', formSelector).click(function(event) {
            event.preventDefault();
            $('input', this).blur();

            if(validate()) {
                $(formSelector).addClass('form-disabled');
                $.ajax({
                    url:  '/forms/submit',
                    type: 'post',
                    data:  $(formSelector).serialize()
                }).then(onSubmitSuccess, onSubmitFailure);
            }
        });

        function chooseResponseMessage() {
            var responseClass = 'high';
            if (useDifferentiatedResponseMessages) {
                var $selectedRadioButton = $(mainFieldSelector + ':checked', formSelector);
                var selectedValue = $selectedRadioButton.closest('.a-radio').data('index');
                responseClass = selectedValue > 3 ? 'high' : 'low';
            }

            $('.response-message', formSelector).attr('data-result', responseClass);
        }

        function clearValidationMessage(field) {
            $(field).closest('.form-field-container').removeAttr('data-error');
        }

        function validate() {
            var isValid = true;
            var fieldContainers = $('.form-field-container.required', formSelector);
            fieldContainers.each(
                function(index) {
                    var numChecked = $('input::checked', this).length;
                    if (numChecked === 0) {
                        $(this).attr('data-error', 'empty');
                        isValid = false;
                    }
                });
            return isValid;
        }

        function onSubmitSuccess(data) {
            if (data.successful) {
                chooseResponseMessage();
                $(formSelector).addClass('submitted');
                $(formSelector).removeClass('form-disabled');
            } else {
                onSubmitFailure();
            }
        }

        function onSubmitFailure(data) {
            $(formSelector).attr('data-error', 'submission');
            $(formSelector).removeClass('form-disabled');
        }

        function setUpMultiChoiceNoneOfAbove() {
            var noneOfAboveClass = 'none-of-above';
            if ($('.' + noneOfAboveClass, formSelector).length === 0) {
                // none-of-above option not present; setup not needed
                return;
            }

            $('input:checkbox', formSelector).change(function(event) {
                if (!(event.target && event.target.checked)) {
                    return;
                }

                var answerContainer = $(event.target).closest('.form-field-answer');
                var fieldContainer = $(event.target).closest('.form-options');
                var allAnswerContainers = fieldContainer.find('.form-field-answer');

                if (answerContainer.hasClass(noneOfAboveClass)) {
                    // none-of-the-above was just selected
                    allAnswerContainers.not('.' + noneOfAboveClass).find(':checkbox:checked').attr('checked', false);
                } else {
                    // something other than none-of-the-above was just selected
                    allAnswerContainers.filter('.' + noneOfAboveClass).find(':checkbox:checked').attr('checked', false);
                }
            });
        }
    });
</script>
